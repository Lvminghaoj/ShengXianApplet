page {
    height: 100%
}

.container {
    width: 100%;
    height: 100%;
    background: rgb(244, 243, 243);
    display: flex;
    align-items: center;
}

.ContainerBox {
    width: 600rpx;
    height: 838rpx;
    background: white;
    margin: 0 auto;
    border-radius: 10rpx;
}

.BoxTop {
    height: 100%;
    margin: 0 50rpx;
}

.BoxCenter {
    height: 253rpx;
    border-bottom: 1rpx solid rgb(102, 102, 102);
}

.BoxCenterWords {
    padding-top: 75rpx;
    color: rgb(102, 102, 102);
    font-size: 40rpx;
}

.BoxCenterWordsOne {
    padding: 40rpx 0;
    color: rgb(102, 102, 102);
    font-size: 40rpx;
}

.BoxCenterWordsTwo {
    padding: 40rpx 0;
    color: rgb(102, 102, 102);
    font-size: 32rpx;
    text-align: center;
}

.LookImg {
    width: 52rpx;
    height: 52rpx;
    margin-right: 18rpx;
}

.BoxCenterImgWords {
    display: flex;
    padding-top: 29rpx;
    color: rgb(102, 102, 102);
    font-size: 40rpx;
    justify-content: space-between;
}

.Img {
    width: 338rpx;
    height: 324rpx;
    margin: 0 auto;
}


