page {
    background: rgba(244, 243, 243, 1);
}

.conBox {
    width: 690rpx;
    height: 100rpx;
    margin: 30rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: white;
}

.boxLeft {
    margin-left: 30rpx;
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
}

.lookImg {
    width: 14rpx;
    height: 22rpx;
}

.boxRight {
    margin-right: 30rpx;
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    display: flex;
    align-items: center;
}

.boxwords {
    margin-right: 26rpx
}

.conBoxOne {
    width: 690rpx;
    height: 300rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 30rpx 30rpx 0rpx 0rpx;
    margin: 0 30rpx;
}

.conBoxTwo {
    display: flex;
    align-items: center;
    padding-top: 10rpx;
    margin: 0 40rpx;
    border-bottom: 1rpx solid rgba(244, 243, 243, 1);
}

.conBoxWords {
    padding-top: 32rpx;
    padding-left: 42rpx;
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
}

.Rnb {
    width: 19rpx;
    height: 23rpx;
    font-size: 30rpx;
    font-family: Adobe Heiti Std;
    font-weight: normal;
    color: rgba(51, 51, 51, 1);
    margin-top: 7rpx;
}

.money {
    font-size: 60rpx;
    font-family: DIN;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    margin-left: 15rpx;
    padding: 10rpx 0;
}

.conBoxThree {
    display: flex;
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    margin: 10rpx 40rpx;
}

.conBoxThreeWords {
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(65, 164, 255, 1);
    margin-left: 19rpx;
}

.cashMoneyTocake {
    width: 690rpx !important;
    height: 88rpx;
    background: rgba(255, 151, 2, 1);
    border-radius: 44rpx;
    margin: 30rpx 20rpx;
    padding: 0;
    font-size: 30rpx;
    font-family: Source Han Sans CN;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 88rpx;
}

.mack {
    font-size: 24rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    text-align: center;
}

.commodity_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9;
    color: #fff;
}

.commodity_attr_box {
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 10;
    background: #fff;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.commodityWords {
    height: 100%;
    margin: 0 20rpx;
}

.commodityWordsFrom {
    padding: 20rpx;
    border-bottom: 1rpx solid rgb(224, 224, 224);
    color: rgb(31, 31, 31);
    font-size: 35rpx;
    text-align: center;
}

.CommodityLookImg {
    width: 50rpx;
    height: 50rpx;
}

.commdityFromWOds {
    color: rgb(78, 80, 85);
    font-size: 32rpx;
    margin-left: 15rpx;
}

.commodityCenter {
    width: 100%;
}

.CommodityFor {
    padding: 20rpx 0;
    display: flex;
    border-bottom: 1rpx solid rgba(244, 243, 243, 1);
    justify-content: space-between;
    align-items: center;
    width: 100%;
}

.commodityBottom {
    width: 100%;
    text-align: center;
    color: #4169E1;
    font-size: 32rpx;
    border-top: 1rpx solid rgb(224, 224, 224);
    line-height: 80rpx;
}

.canvas {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 999999rpx;
    background-color: rgba(0, 0, 0, 0.3)
}

.lookImgTrue {
    width: 35rpx;
    height: 35rpx;
    margin-top: 10rpx;
}