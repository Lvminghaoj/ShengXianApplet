.InputLook {
    height: 100%;
    font-size: 28rpx;
    color: rgb(51, 51, 51);
    padding-left: 20rpx;
    border: 1px solid orange;
    border-radius: 40rpx;
}

icon {
    position: absolute;
    top: 14rpx;
    left: 30rpx;
    width: 26rpx;
    height: 26rpx;
}

.ContarBoxLookSou {
    border-radius: 10rpx;
    background: white;
    display: flex;
    position: relative;
}

.InputChange {
    width: 650rpx;
    height: 60rpx;
    margin: 15rpx 10rpx;
    border-radius: 40rpx;
    position: relative;
}

.search {
    font-size: 26rpx;
    color: rgb(51, 51, 51);
    line-height: 90rpx;
}

.searchLook {
    padding: 15rpx;
    background: rgb(244, 243, 243);
}

.searchLookTop {
    color: rgb(51, 51, 51);
    font-size: 28rpx;
    font-weight: 600;
}

.searchLookCenter {
    padding: 10rpx;
    width: 650rpx;
    border-radius: 30rpx;
    display: flex;
    flex-wrap: wrap;
}

.searchBox {
    border-radius: 40rpx;
    padding: 5rpx 20rpx;
    color: rgb(51, 51, 51);
    font-size: 13px;
    background: #ccc;
    margin: 10rpx;
}

.lookImg {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 16rpx;
    right: 18rpx;
    z-index: 999999;
}

.loginTo {
    width: 30rpx;
    height: 30rpx;
}

.box {
    margin: 0 20rpx;
}

.boxCenter {
    border-bottom: 1rpx solid #ccc;
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
}